	},
	"homepage": "https://github.com/onecentlin/laravel-blade-snippets-vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/onecentlin/laravel-blade-snippets-vscode"
	},
	"bugs": {
		"url": "https://github.com/onecentlin/laravel-blade-snippets-vscode/issues"

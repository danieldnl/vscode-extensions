	"engines": {
		"vscode": "^1.0.0"
	},
	"keywords": [
		"laravel",
		"blade",
		"template"
	],
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#f66f62",
		"theme": "dark"
	},
	"categories": [
		"Languages",
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "blade",
				"aliases": [
					"Laravel Blade",
					"Blade",
					"blade"
				],
				"extensions": [
					".blade.php"
				],
				"configuration": "./blade.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "blade",
				"scopeName": "text.html.php.laravel-blade",
				"path": "./syntaxes/blade.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "blade",
				"path": "./snippets/snippets.json"
			},
			{
				"language": "blade",
				"path": "./snippets/helpers.json"
			}
		]
	},
